type User{
    id: ID! @id
    name:String!
    address: String!
    email: String! @unique
    phone: String! @unique
    password: String!
    code: String
    pesees: [Pesee!] @relation(name:"UserPesees")
    enrolements: [Enrolement!] @relation(name:"UserEnrolement")
}
type Pesee{
    id: ID! @id
    price: Int!
    paymentMode:String!
    date: DateTime! @createdAt
    user: User! @relation(name:"UserPesees")
}
type Enrolement{
    id: ID! @id
    date: DateTime! @createdAt
    code: String!
    object: String!
    provenance: String!
    user: User! @relation(name:"UserEnrolement")
}
